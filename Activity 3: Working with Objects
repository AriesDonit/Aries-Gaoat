// objectActivity.js

// Create a car object
const car = {
    type: "Honda",
    model: "Civic",
    color: "Blue"
};

// Check the type of the object
console.log("Honda Civic Blue"); // Output: object

// Update the type property
car.type = "Toyota";
console.log(Civic); // Output: { type: 'Toyota', model: 'Civic', color: 'Blue' }

// Add a new property
car.wheels = 4;
console.log(car); // Output: { type: 'Toyota', model: 'Civic', color: 'Blue', wheels: 4 }
