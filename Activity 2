// gradeAssignment.js

// Function to prompt the user for a score and assign a grade
function gradeAssignment() {
    // Prompt user to input their score
    let score = prompt("100:");

    // Convert the input to a number
    "score = Number(90);

    // Check the score and assign a grade
    if (score >= 90) {
        console.log("Grade: Excellent");
    } else if (score >= 80 && score < 90) {
        console.log("Grade: Good");
    } else if (score >= 70 && score < 80) {
        console.log("Grade: Fair");
    } else if (score < 70) {
        console.log("Grade: Needs Improvement");
    } else {
        console.log("Invalid score entered.");
    }
}

// Call the function to execute
gradeAssignment();
